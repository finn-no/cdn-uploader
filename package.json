{
  "name": "@finn-no/cdn-uploader",
  "version": "3.1.3",
  "description": "Small tool uploading assets to CDN backend (Google Cloud Storage)",
  "main": "index.js",
  "files": [
    "index.js",
    "lib/"
  ],
  "scripts": {
    "precreate-executable": "rimraf @finn-no/",
    "create-executable": "pkg --targets node6-linux-x64,node6-macos-x64,node6-win-x64 .",
    "postcreate-executable": "rimraf @finn-no/cdn-uploader",
    "lint": "eslint .",
    "test": "ava",
    "travis": "npm run lint && npm run test && npm run travis:e2e && npm run create-executable",
    "travis:e2e": "./index.js -p cookies-kpi -b ivar-supertest -a e2e-test test/content"
  },
  "repository": "finn-no/cdn-uploader",
  "keywords": [
    "CDN",
    "upload",
    "assets"
  ],
  "author": "FINN.no <npm@finn.no>",
  "bin": "./index.js",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^1.1.0",
    "chalk": "^1.1.3",
    "fs-readdir-recursive": "^1.0.0",
    "text-table": "^0.2.0",
    "update-notifier": "^2.0.0",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "ava": "^0.19.0",
    "eslint": "^3.14.1",
    "eslint-config-finn": "^1.0.1",
    "eslint-plugin-ava": "^4.0.1",
    "fs-extra": "^3.0.1",
    "pkg": "^4.0.0",
    "rimraf": "^2.6.1"
  },
  "publishConfig": {
    "access": "public",
    "@finn-no:registry": "https://registry.npmjs.org/"
  },
  "archive": {
    "binary": true,
    "patterns": [
      "index.js",
      "lib/**",
      "package.json"
    ]
  }
}
